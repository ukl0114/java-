package test1.codingTest2;

public class test1 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int apple = 5;
		int pear = 6;
		int orange = 7;
		int total = apple + pear + orange;
		float one_day = total / 24;
		
		System.out.println("과수원에서 하루에생산되는 과일 총 개수 : " + total);
		System.out.println("시간당 생산량 : " + one_day);
	}

}
