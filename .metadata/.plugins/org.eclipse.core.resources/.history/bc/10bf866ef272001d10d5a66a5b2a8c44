package test1.codingTest7;

public class Test2 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		System.out.println("숫자를 입력해주세요.");
		int a = sc.nextInt();
		System.out.println("숫자를 입력해주세요.");
		int b = sc.nextInt();
		System.out.println("숫자를 입력해주세요.");
		int c = sc.nextInt();
		System.out.println("숫자를 입력해주세요.");
		int d = sc.nextInt();
		
		int ab = a+b;
		int cd = c+d;
		
		if (ab > cd)
			System.out.println("True");
		else
			System.out.println("False");
	}

}
